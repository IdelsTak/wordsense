    <groupId>com.github.idelstak</groupId>
    <artifactId>wordsense-app</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <modules>
        <module>wordsense-core</module>
        <module>wordsense-ui</module>
        <module>wordsense-api</module>
        <module>wordsense-main</module>
    </modules>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
</project>